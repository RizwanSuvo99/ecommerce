{
  "name": "@ecommerce/types",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./auth": {
      "types": "./src/auth.ts",
      "default": "./src/auth.ts"
    },
    "./product": {
      "types": "./src/product.ts",
      "default": "./src/product.ts"
    },
    "./order": {
      "types": "./src/order.ts",
      "default": "./src/order.ts"
    },
    "./user": {
      "types": "./src/user.ts",
      "default": "./src/user.ts"
    },
    "./common": {
      "types": "./src/common.ts",
      "default": "./src/common.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "devDependencies": {
    "@ecommerce/tsconfig": "workspace:*",
    "typescript": "^5.3.3"
  }
}
